{
  "name": "faucet",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "generate-typechain": "typechain --target=ethers-v5 'abi/*.json'",
    "generate-graphql": "graphql-codegen",
    "generate-prisma": "prisma generate",
    "generate": "yarn generate-typechain && yarn generate-prisma && yarn generate-graphql",
    "postinstall": "yarn generate",
    "start": "node dist/src/server",
    "listener": "npx ts-node src/blockchain_listener/main.ts",
    "migrate:dev": "yarn prisma migrate dev",
    "migrate:prod": "yarn prisma migrate deploy"
  },
  "dependencies": {
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/typescript": "^2.4.11",
    "@graphql-codegen/typescript-resolvers": "^2.6.4",
    "@prisma/client": "^3.14.0",
    "@types/node": "^17.0.36",
    "apollo-server": "^3.5.0",
    "dotenv": "^10.0.0",
    "ethers": "^5.5.1",
    "graphql": "^16.0.1",
    "jsonwebtoken": "^8.5.1",
    "ts-node": "^10.4.0",
    "twitter-api-v2": "^1.12.2",
    "typescript": "^4.4.4"
  },
  "devDependencies": {
    "@typechain/ethers-v5": "^8.0.5",
    "prisma": "^3.14.0",
    "typechain": "^6.0.5"
  }
}
